<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.parammgr.db.entity.DBStruct" table="DBStruct">
		<cache usage="read-write" />
		<composite-id name="primaryKey" class="com.parammgr.db.entity.DBStructPK">
			<key-property name="projectId" column="projectId" type="string"></key-property>
			<key-property name="structId" column="structId" type="int"></key-property>
		</composite-id>

		<property name="structName" />
		<property name="tempTable" />
		<property name="createDate" />

		<set name="elements" lazy="false" inverse="false" cascade="all">
			<key>
				<column name="projectId" />
				<column name="structId" />
			</key>
			<one-to-many class="com.parammgr.db.entity.DBStructElement" />
		</set>
	</class>

</hibernate-mapping>